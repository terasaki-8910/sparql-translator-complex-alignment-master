PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX agro: <http://ontology.irstea.fr/agronomictaxon/core#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skosxl: <http://www.w3.org/2008/05/skos-xl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX agronto: <http://aims.fao.org/aos/agrontology#>
PREFIX agrovoc: <http://aims.fao.org/aos/agrovoc/>


select ?commonName where {
?taxon rdf:type skos:Concept.
{?taxon skos:prefLabel ?label. }UNION
{?taxon skosxl:prefLabel ?var_temp0.
?var_temp0 skosxl:literalForm ?label.
}UNION
{?taxon skos:altLabel ?label. }UNION
{?taxon skosxl:altLabel ?var_temp1.
?var_temp1 skosxl:literalForm ?label.
}UNION
{?taxon skos:altLabel ?label. }
{?taxon skos:prefLabel ?commonName. }UNION
{?taxon skosxl:prefLabel ?var_temp2.
?var_temp2 skosxl:literalForm ?commonName.
}UNION
{?taxon skos:altLabel ?commonName. }UNION
{?taxon skosxl:altLabel ?var_temp3.
?var_temp3 skosxl:literalForm ?commonName.
}UNION
{?taxon skos:altLabel ?commonName. }
filter (regex(?label,"^triticum$", "i") && lang(?commonName) ="fr").
}
